   "source": [
    "import numpy as np\n",
    "\n",
    "N=10\n",
    "A=np.zeros([N,N])\n",
    "for i in range(N):\n",
    "    A[i,i]=6\n",
    "    if i+1<N:\n",
    "        A[i,i+1]=1.25\n",
    "    if i+2<N:\n",
    "        A[i,i+2]=1.25\n",
    "    for j in range(N):\n",
    "        if(np.abs(i-j)>=3):\n",
    "            A[i,j]=0"
   ]
